# Comma separated values (CSV format)
id, title, completed
1, Learn HTML, true
2, Learn CSS, false
3, Learn JS, false

# XML eXtended Markup Language

# JSON (JavaScript Object Notation)
[
    {
        "id": 1,
        "title": "Learn HTML",
        "completed": true
    },
    {
        "id": 2,
        "title": "Learn CSS",
        "completed": false
    },
    {
        "id": 3,
        "title": "Learn JS",
        "completed": false
    },
]